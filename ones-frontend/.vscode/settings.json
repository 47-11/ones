{
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "vue"
    ],
    "cSpell.words": [
        "Openapi",
        "Webservice",
        "compat",
        "fontawesome",
        "fortawesome",
        "tailwindcss",
        "vuex"
    ],
    "javascript.preferences.quoteStyle": "double",
    "typescript.preferences.quoteStyle": "double",
    "vetur.validation.style": false,
    "i18n-ally.localesPaths": [
        "src/assets/i18n"
    ],
    "i18n-ally.keystyle": "nested",
    "i18n-ally.usage.scanningIgnore": ["target/**"]
}
